@article{Becker:2014aa,
	Adscomment = {9 pages, 7 figures, to be submitted, comments welcome!},
	Adsurl = {http://adsabs.harvard.edu/cgi-bin/bib_query?arXiv:1412.3851},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1412.3851v1},
	Author = {{Becker}, Matthew~R. and {Rozo}, Eduardo},
	Date-Added = {2014-12-15 07:34:56 +0000},
