	Journal = {International Journal of Modern Physics E},
	Keywords = {Nuclear matter, neutron matter, isospin asymmetric matter, symmetry energy, neutron stars, Asymmetric matter, neutron matter, Mass and neutron distributions, Equations of state of nuclear matter, Nuclear matter aspects of neutron stars},
	Month = {01},
