	Adsurl = {http://adsabs.harvard.edu/abs/2015arXiv150103014C},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1501.03014},
	Author = {{Camelio}, G. and {Lombardi}, M.},
