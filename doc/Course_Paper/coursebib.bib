@article{Hall:2014aa,
	Adscomment = {5 pages, 4 figures; submitted to MNRAS Letters},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2014arXiv1401.6018H},
