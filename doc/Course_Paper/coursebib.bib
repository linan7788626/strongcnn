	Author = {{Rhodes}, J.~D. and {Massey}, R.~J. and {Albert}, J. and {Collins}, N. and {Ellis}, R.~S. and {Heymans}, C. and {Gardner}, J.~P. and {Kneib}, J.-P. and {Koekemoer}, A. and {Leauthaud}, A. and {Mellier}, Y. and {Refregier}, A. and {Taylor}, J.~E. and {Van Waerbeke}, L.},
