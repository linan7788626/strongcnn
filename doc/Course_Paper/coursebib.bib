	Adsurl = {http://adsabs.harvard.edu/abs/2012MNRAS.427..146H},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1210.0032},
	Author = {{Heymans}, C. and {Van Waerbeke}, L. and {Miller}, L. and {Erben}, T. and {Hildebrandt}, H. and {Hoekstra}, H. and {Kitching}, T.~D. and {Mellier}, Y. and {Simon}, P. and {Bonnett}, C. and {Coupon}, J. and {Fu}, L. and {Harnois D{\'e}raps}, J. and {Hudson}, M.~J. and {Kilbinger}, M. and {Kuijken}, K. and {Rowe}, B. and {Schrabback}, T. and {Semboloni}, E. and {van Uitert}, E. and {Vafaei}, S. and {Velander}, M.},
