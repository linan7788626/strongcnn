@article{Schneider:2014ac,
	Adscomment = {4 pages, submitted to A&A (Letters)},
	Adsurl = {http://adsabs.harvard.edu/cgi-bin/bib_query?arXiv:1406.6152},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1406.6152v1},
	Author = {{Schneider}, Peter},
	Date-Added = {2014-06-25 08:16:58 +0000},
