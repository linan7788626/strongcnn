	Bdsk-Url-1 = {http://arxiv.org/abs/1501.03848v1},
	Bdsk-Url-2 = {http://adsabs.harvard.edu/cgi-bin/bib_query?arXiv:1501.03848}}

@article{Raccanelli:2015aa,
	Adscomment = {19 pages, 4 figures. This article is part of the 'Cosmology Chapter, Advancing Astrophysics with the SKA (AASKA14), Conference, Giardini Naxos (Italy), June 9th-13th 2014'},
	Adsurl = {http://adsabs.harvard.edu/cgi-bin/bib_query?arXiv:1501.03821},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1501.03821v1},
	Author = {{Raccanelli}, Alvise and {Bull}, Philip and {Camera}, Stefano and {Bacon}, David and {Blake}, Chris and {Dore}, Olivier and {Ferreira}, Pedro and {Maartens}, Roy and {Santos}, Mario and {Viel}, Matteo and {Zhao}, Gong-bo},
	Date-Added = {2015-01-19 03:16:40 +0000},
