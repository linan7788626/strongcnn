	Title = {{Trends of Dark Matter Halos with Luminosity, Color, and Redshift in the Deep Lens Survey}},
	Volume = 41,
	Year = 2009,
	Abstract = {We present the first results from a galaxy-galaxy weak gravitational lensing analysis of data from the Deep Lens Survey (DLS; http://dls.physics.ucdavis.edu/), a deep optical survey of five 4 square degree fields. Utilizing photometric redshifts derived from the four DLS filters (BVRz), we investigate the correlations between average shear profiles and galaxy properties such as luminosity and color. We also examine the redshift evolution of these trends. Sample selection effects are simulated and compared with observations.}}

@article{Navarro:1997aa,
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/1997ApJ...490..493N},
	Author = {{Navarro}, J.~F. and {Frenk}, C.~S. and {White}, S.~D.~M.},
