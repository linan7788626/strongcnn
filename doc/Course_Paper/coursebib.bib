@article{Prada:2003ai,
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2003ApJ...598..260P},
	Author = {{Prada}, F. and {Vitvitska}, M. and {Klypin}, A. and {Holtzman}, J.~A. and {Schlegel}, D.~J. and {Grebel}, E.~K. and {Rix}, H.-W. and {Brinkmann}, J. and {McKay}, T.~A. and {Csabai}, I.},
