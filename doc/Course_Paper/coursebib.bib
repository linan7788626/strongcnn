	Author = {{Sevilla}, I. and {Armstrong}, R. and {Bertin}, E. and {Carlson}, A. and {Daues}, G. and {Desai}, S. and {Gower}, M. and {Gruendl}, R. and {Hanlon}, W. and {Jarvis}, M. and {Kessler}, R. and {Kuropatkin}, N. and {Lin}, H. and {Marriner}, J. and {Mohr}, J. and {Petravick}, D. and {Sheldon}, E. and {Swanson}, M.~E.~C. and {Tomashek}, T. and {Tucker}, D. and {Yang}, Y. and {Yanny}, B. and {for the DES Collaboration}},
