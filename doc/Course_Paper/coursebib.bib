	Bdsk-Url-1 = {http://dx.doi.org/10.1111/j.1365-2966.2012.21624.x},
	Bdsk-Url-2 = {http://arXiv.org/abs/1202.2332},
	Bdsk-Url-3 = {http://adsabs.harvard.edu/abs/2012MNRAS.426.1262H}}

