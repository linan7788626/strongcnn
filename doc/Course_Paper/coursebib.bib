	Arxivurl = {http://arXiv.org/abs/1205.3788},
	Author = {{Gonzalez}, A.~H. and {Stanford}, S.~A. and {Brodwin}, M. and {Fedeli}, C. and {Dey}, A. and {Eisenhardt}, P.~R.~M. and {Mancone}, C. and {Stern}, D. and {Zeimann}, G.},
	Date-Added = {2013-01-27 02:04:39 +0000},
