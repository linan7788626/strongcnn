	Author = {{Heymans}, C. and {Van Waerbeke}, L. and {Bacon}, D. and {Berge}, J. and {Bernstein}, G. and {Bertin}, E. and {Bridle}, S. and {Brown}, M.~L. and {Clowe}, D. and {Dahle}, H. and {Erben}, T. and {Gray}, M. and {Hetterscheidt}, M. and {Hoekstra}, H. and {Hudelot}, P. and {Jarvis}, M. and {Kuijken}, K. and {Margoniner}, V. and {Massey}, R. and {Mellier}, Y. and {Nakajima}, R. and {Refregier}, A. and {Rhodes}, J. and {Schrabback}, T. and {Wittman}, D.},
	Date-Added = {2013-06-28 00:48:06 +0000},
