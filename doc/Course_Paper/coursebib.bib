	Bdsk-Url-1 = {http://dx.doi.org/10.1111/j.1365-2966.2011.20249.x},
	Bdsk-Url-2 = {http://adsabs.harvard.edu/abs/2012MNRAS.420.3240N},
	Bdsk-Url-3 = {http://arXiv.org/abs/1107.1395}}

@article{Sheldon:2012aa,
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2012ApJS..201...32S},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1109.5192},
	Author = {{Sheldon}, E.~S. and {Cunha}, C.~E. and {Mandelbaum}, R. and {Brinkmann}, J. and {Weaver}, B.~A.},
	Date-Added = {2013-01-27 02:07:31 +0000},
	Date-Modified = {2013-01-27 02:07:31 +0000},
	Doi = {10.1088/0067-0049/201/2/32},
	Eid = {32},
	Eprint = {1109.5192},
	Journal = {\apjs},
	Keywords = {cosmology: observations, galaxies: distances and redshifts, large-scale structure of universe},
	Month = aug,
	Pages = {32},
	Primaryclass = {astro-ph.CO},
	Title = {{Photometric Redshift Probability Distributions for Galaxies in the SDSS DR8}},
	Volume = 201,
	Year = 2012,
	Abstract = {We present redshift probability distributions for galaxies in the Sloan Digital Sky Survey (SDSS) Data Release 8 imaging data. We used the nearest-neighbor weighting algorithm to derive the ensemble redshift distribution N(z), and individual redshift probability distributions P(z) for galaxies with r < 21.8 and u < 29.0. As part of this technique, we calculated weights for a set of training galaxies with known redshifts such that their density distribution in five-dimensional color-magnitude space was proportional to that of the photometry-only sample, producing a nearly fair sample in that space. We estimated the ensemble N(z) of the photometric sample by constructing a weighted histogram of the training-set redshifts. We derived P(z)'s for individual objects by using training-set objects from the local color-magnitude space around each photometric object. Using the P(z) for each galaxy can reduce the statistical error in measurements that depend on the redshifts of individual galaxies. The spectroscopic training sample is substantially larger than that used for the DR7 release. The newly added PRIMUS catalog is now the most important training set used in this analysis by a wide margin. We expect the primary sources of error in the N(z) reconstruction to be sample variance and spectroscopic failures: The training sets are drawn from relatively small volumes of space, and some samples have large incompleteness. Using simulations we estimated the uncertainty in N(z) due to sample variance at a given redshift to be ~10%-15%. The uncertainty on calculations incorporating N(z) or P(z) depends on how they are used; we discuss the case of weak lensing measurements. The P(z) catalog is publicly available from the SDSS Web site.},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1088/0067-0049/201/2/32},
	Bdsk-Url-2 = {http://adsabs.harvard.edu/abs/2012ApJS..201...32S},
	Bdsk-Url-3 = {http://arXiv.org/abs/1109.5192}}

