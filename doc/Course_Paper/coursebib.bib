@article{Carlstrom:2002aa,
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2002ARA%26A..40..643C},
	Arxivurl = {http://arXiv.org/abs/astro-ph/0208192},
	Author = {{Carlstrom}, J.~E. and {Holder}, G.~P. and {Reese}, E.~D.},
	Date-Added = {2013-06-20 08:25:51 +0000},
	Date-Modified = {2013-06-20 08:25:51 +0000},
	Doi = {10.1146/annurev.astro.40.060401.093803},
	Eprint = {arXiv:astro-ph/0208192},
	Journal = {\araa},
	Keywords = {galaxy clusters, cosmic microwave background, structure formation, surveys},
	Pages = {643-680},
	Title = {{Cosmology with the Sunyaev-Zel'dovich Effect}},
	Volume = 40,
	Year = 2002,
	Abstract = {The Sunyaev-Zel'dovich effect (SZE) provides a unique way to map the large-scale structure of the universe as traced by massive clusters of galaxies. As a spectral distortion of the cosmic microwave background, the SZE is insensitive to the redshift of the galaxy cluster, making it well-suited for studies of clusters at all redshifts, and especially at reasonably high redshifts (z > 1) where the abundance of clusters is critically dependent on the underlying cosmology. Recent high signal-to-noise detections of the SZE have enabled interesting constraints on the Hubble constant and on the matter density of the universe using small samples of galaxy clusters. Upcoming SZE surveys are expected to find hundreds to thousands of new galaxy clusters, with a mass selection function that is remarkably uniform with redshift. In this review we provide an overview of the SZE and its use for cosmological studies, with emphasis on the cosmology that can, in principle, be extracted from SZE survey yields. We discuss the observational and theoretical challenges that must be met before precise cosmological constraints can be extracted from the survey yields.},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1146/annurev.astro.40.060401.093803},
	Bdsk-Url-2 = {http://arXiv.org/abs/astro-ph/0208192},
	Bdsk-Url-3 = {http://adsabs.harvard.edu/abs/2002ARA%26A..40..643C}}

