@article{Chan:2013aa,
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2013PhRvD..88j3501C},
	Archiveprefix = {arXiv},
