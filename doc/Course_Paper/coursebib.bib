	Author = {{Codis}, Sandrine and {Gavazzi}, Raphael and {Dubois}, Yohan and {Pichon}, Christophe and {Benabed}, Karim and {Desjacques}, Vincent and {Pogosyan}, Dmitry and {Devriendt}, Julien and {Slyz}, Adrianne},
	Date-Added = {2014-06-19 03:41:24 +0000},
