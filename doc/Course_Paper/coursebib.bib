@article{Cautun:2014aa,
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2014MNRAS.441.2923C},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1401.7866},
	Author = {{Cautun}, M. and {van de Weygaert}, R. and {Jones}, B.~J.~T. and {Frenk}, C.~S.},
	Date-Added = {2014-06-18 19:39:28 +0000},
