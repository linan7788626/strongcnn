	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1310.3880},
	Author = {{Angulo}, R.~E. and {Baugh}, C.~M. and {Frenk}, C.~S. and {Lacey}, C.~G.},
	Date-Added = {2013-10-22 16:47:10 +0000},
