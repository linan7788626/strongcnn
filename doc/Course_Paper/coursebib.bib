	Arxivurl = {http://arXiv.org/abs/0904.0382},
	Author = {{Bolotin}, Y.~L. and {Cherkaskiy}, V.~A. and {Ivashkevych}, G.~I. and {Lemets}, O.~A. and {Tanatarov}, I.~V. and {Yerokhin}, D.~A.},
