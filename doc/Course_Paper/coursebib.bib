	Bdsk-Url-1 = {http://dx.doi.org/10.1088/0264-9381/27/11/114002}}

@article{Demorest:2010aa,
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2010Natur.467.1081D},
	Archiveprefix = {arXiv},
	Author = {{Demorest}, P.~B. and {Pennucci}, T. and {Ransom}, S.~M. and {Roberts}, M.~S.~E. and {Hessels}, J.~W.~T.},
	Date-Added = {2012-04-15 21:55:50 -0400},
	Date-Modified = {2014-09-23 02:09:56 +0000},
	Doi = {10.1038/nature09466},
	Eprint = {1010.5788},
	Journal = {\nat},
	Month = oct,
	Pages = {1081-1083},
	Primaryclass = {astro-ph.HE},
	Read = {1},
	Readabstract = {1},
	Title = {{A two-solar-mass neutron star measured using Shapiro delay}},
	Volume = 467,
	Year = 2010,
	Abstract = {Neutron stars are composed of the densest form of matter known to exist in our Universe, the composition and properties of which are still theoretically uncertain. Measurements of the masses or radii of these objects can strongly constrain the neutron star matter equation of state and rule out theoretical models of their composition. The observed range of neutron star masses, however, has hitherto been too narrow to rule out many predictions of `exotic' non-nucleonic components. The Shapiro delay is a general-relativistic increase in light travel time through the curved space-time near a massive body. For highly inclined (nearly edge-on) binary millisecond radio pulsar systems, this effect allows us to infer the masses of both the neutron star and its binary companion to high precision. Here we present radio timing observations of the binary millisecond pulsar J1614-2230 that show a strong Shapiro delay signature. We calculate the pulsar mass to be (1.97+/-0.04)Msolar, which rules out almost all currently proposed hyperon or boson condensate equations of state (Msolar, solar mass). Quark matter can support a star this massive only if the quarks are strongly interacting and are therefore not `free' quarks.},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1038/nature09466}}

@article{Lattimer:2010aa,
