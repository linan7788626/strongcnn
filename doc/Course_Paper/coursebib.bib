@article{Howlett:2014aa,
	Adscomment = {18 pages, 22 figures, submitted to MNRAS},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2014arXiv1409.3238H},
	Archiveprefix = {arXiv},
