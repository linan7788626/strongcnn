	Bdsk-Url-1 = {http://dx.doi.org/10.1086/306225},
	Bdsk-Url-2 = {http://arXiv.org/abs/astro-ph/9711124},
	Bdsk-Url-3 = {http://adsabs.harvard.edu/abs/1998ApJ...506...64S}}

