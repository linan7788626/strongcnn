	Author = {{Sonnenfeld}, A. and {Treu}, T. and {Gavazzi}, R. and {Suyu}, S.~H. and {Marshall}, P.~J. and {Auger}, M.~W. and {Nipoti}, C.},
	Date-Added = {2014-10-30 16:58:19 +0000},
	Date-Modified = {2014-10-30 16:58:20 +0000},
	Doi = {10.1088/0004-637X/777/2/98},
	Eid = {98},
