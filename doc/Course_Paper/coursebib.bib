	Author = {{Percival}, W.~J. and {Ross}, A.~J. and {S{\'a}nchez}, A.~G. and {Samushia}, L. and {Burden}, A. and {Crittenden}, R. and {Cuesta}, A.~J. and {Magana}, M.~V. and {Manera}, M. and {Beutler}, F. and {Chuang}, C.-H. and {Eisenstein}, D.~J. and {Ho}, S. and {McBride}, C.~K. and {Montesano}, F. and {Padmanabhan}, N. and {Reid}, B. and {Saito}, S. and {Schneider}, D.~P. and {Seo}, H.-J. and {Tojeiro}, R. and {Weaver}, B.~A.},
