	Bdsk-Url-1 = {http://dx.doi.org/10.1088/0004-637X/721/1/456},
	Bdsk-Url-2 = {http://arXiv.org/abs/1003.0687},
	Bdsk-Url-3 = {http://adsabs.harvard.edu/abs/2010ApJ...721..456M}}

@article{Newman:2008aa,
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
