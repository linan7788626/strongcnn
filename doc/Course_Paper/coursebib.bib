	Author = {{Rasmussen}, Andrew and {Antilogus}, Pierre and {Astier}, Pierre and {Claver}, Chuck and {Doherty}, Peter and {Dubois-Felsmann}, Gregory and {Gilmore}, Kirk and {Kahn}, Steven and {Kotov}, Ivan and {Lupton}, Robert and {O'Connor}, Paul and {Nomerotski}, Andrei and {Ritz}, Steve and {Stubbs}, Christopher},
	Date-Added = {2014-07-23 15:07:30 +0000},
