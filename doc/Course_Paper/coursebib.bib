@article{Lodge:1919aa,
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/1919Natur.104..354L},
	Author = {{Lodge}, O.~J.},
	Date-Added = {2012-03-10 14:17:02 -0500},
