@article{Yoo:2014aa,
	Adscomment = {21 pages, no figures. Invited review article, accepted for publication in Classical and Quantum Gravity focus issue on 'Relativistic Effects in Cosmology', edited by Kazuya Koyama},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2014arXiv1409.3223Y},
