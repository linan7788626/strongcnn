	Bdsk-Url-3 = {http://adsabs.harvard.edu/abs/2013ApJ...763...18B}}

@article{Wu:2013aa,
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2013ApJ...767...23W},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1210.6358},
	Author = {{Wu}, H.-Y. and {Hahn}, O. and {Wechsler}, R.~H. and {Behroozi}, P.~S. and {Mao}, Y.-Y.},
	Date-Added = {2013-04-29 20:11:40 +0000},
	Date-Modified = {2014-09-23 02:09:58 +0000},
	Doi = {10.1088/0004-637X/767/1/23},
	Eid = {23},
