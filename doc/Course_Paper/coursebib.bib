	Arxivurl = {http://arXiv.org/abs/1307.2566},
	Author = {{Smith}, M. and {Bacon}, D.~J. and {Nichol}, R.~C. and {Campbell}, H. and {Clarkson}, C. and {Maartens}, R. and {D'Andrea}, C.~B. and {Bassett}, B.~A. and {Cinabro}, D. and {Finley}, D.~A. and {Frieman}, J.~A. and {Galbany}, L. and {Garnavich}, P.~M. and {Olmstead}, M.~D. and {Schneider}, D.~P. and {Shapiro}, C. and {Sollerman}, J.},
	Date-Added = {2014-06-18 19:59:33 +0000},
