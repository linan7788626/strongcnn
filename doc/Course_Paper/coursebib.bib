	Author = {{Giocoli}, Carlo and {Metcalf}, R.~Benton and {Baldi}, Marco and {Meneghetti}, Massimo and {Moscardini}, Lauro and {Petkova}, Margarita and {Astronomia}, Dipartimento~di~Fisica~e and {Bologna}, Alma~Mater~Studiorum~Universit{\`a}~di and {Bologna}, INAF~-~Osservatorio~Astronomico~di and {Bologna}, INFN~-~Sezione~di and {Universit{\'e}}, Aix~Marseille and {CNRS}, and {LAM}, and {France}, and {Laboratory}, Jet~Propulsion and {Physics}, Department~of and {Ludwig-Maximilians-Universitaet)},},
