	Author = {{Gruen}, D. and {Seitz}, S. and {Kosyra}, R. and {Brimioulle}, F. and {Koppenhoefer}, J. and {Lee}, C.-H. and {Bender}, R. and {Riffeser}, A. and {Eichner}, T. and {Weidinger}, T. and {Bierschenk}, M.},
	Date-Added = {2014-01-17 05:30:21 +0000},
