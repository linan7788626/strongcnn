@article{Bartelmann:2012aa,
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2012A%26A...547A..98B},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1103.5923},
	Author = {{Bartelmann}, M. and {Viola}, M. and {Melchior}, P. and {Sch{\"a}fer}, B.~M.},
	Date-Added = {2013-01-27 02:06:16 +0000},
