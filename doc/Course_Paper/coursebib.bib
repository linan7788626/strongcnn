@article{Carron:2014ab,
	Adscomment = {6 pages, 1 fig, submitted to MNRAS. comments always welcome},
	Adsurl = {http://adsabs.harvard.edu/cgi-bin/bib_query?arXiv:1412.5511},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1412.5511v1},
	Author = {{Carron}, Julien and {Wolk}, Melody and {Szapudi}, Istv{\'a}n},
	Date-Added = {2014-12-18 08:10:13 +0000},
