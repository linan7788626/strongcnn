@article{Meade:2014aa,
	Adscomment = {19 pages, 15 figures, accepted for publication in PASA (Publications of the Astronomical Society of Australia)},
	Adsurl = {http://adsabs.harvard.edu/cgi-bin/bib_query?arXiv:1407.4881},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1407.4881v1},
	Author = {{Meade}, Bernard~F and {Fluke}, Christopher~J and {Manos}, Steven and {Sinnott}, Richard~O},
