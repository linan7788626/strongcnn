	Bdsk-Url-1 = {http://dx.doi.org/10.1111/j.1365-2966.2012.20923.x},
	Bdsk-Url-2 = {http://adsabs.harvard.edu/abs/2012MNRAS.423..856S},
	Bdsk-Url-3 = {http://arXiv.org/abs/1109.6020}}

