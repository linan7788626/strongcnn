	Bdsk-Url-1 = {http://dx.doi.org/10.1111/j.1365-2966.2008.12947.x},
	Bdsk-Url-2 = {http://arXiv.org/abs/0709.1692},
	Bdsk-Url-3 = {http://adsabs.harvard.edu/abs/2008MNRAS.386..781M}}

