	Archiveprefix = {arXiv},
	Author = {{Tinker}, J.~L. and {Sheldon}, E.~S. and {Wechsler}, R.~H. and {Becker}, M.~R. and {Rozo}, E. and {Zu}, Y. and {Weinberg}, D.~H. and {Zehavi}, I. and {Blanton}, M.~R. and {Busha}, M.~T. and {Koester}, B.~P.},
	Date-Added = {2012-01-03 19:19:31 -0500},
	Date-Modified = {2014-09-23 02:09:57 +0000},
	Doi = {10.1088/0004-637X/745/1/16},
	Eid = {16},
