	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1405.7973},
	Author = {{Castignani}, G. and {Chiaberge}, M. and {Celotti}, A. and {Norman}, C.},
	Date-Added = {2014-06-18 17:57:33 +0000},
	Date-Modified = {2014-09-24 20:17:47 +0000},
