@article{Tyson:1984qf,
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/1984ApJ...281L..59T},
	Author = {{Tyson}, J.~A. and {Valdes}, F. and {Jarvis}, J.~F. and {Mills}, Jr., A.~P.},
	Date-Added = {2011-07-07 16:17:19 -0400},
