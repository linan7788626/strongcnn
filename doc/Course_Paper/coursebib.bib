@article{Nussbaumer:2014aa,
	Adscomment = {10 pages, 3 figures. To appear in the Gedenkband 'In memoriam Hilmar W. Duerbeck'.(Acta Historica Astronomiae)},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
