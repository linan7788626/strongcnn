@article{Chisari:2014ab,
	Adscomment = {30 pages, 16 figures, submitted to MNRAS},
	Adsurl = {http://adsabs.harvard.edu/cgi-bin/bib_query?arXiv:1407.4813},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1407.4813v1},
	Author = {{Chisari}, Nora~Elisa and {Mandelbaum}, Rachel and {Strauss}, Michael~A. and {Huff}, Eric and {Bahcall}, Neta},
	Date-Added = {2014-07-21 01:48:03 +0000},
