	Arxivurl = {http://arXiv.org/abs/1306.6870},
	Author = {{Duncan}, C.~A.~J. and {Joachimi}, B. and {Heavens}, A.~F. and {Heymans}, C. and {Hildebrandt}, H.},
	Date-Added = {2014-06-18 19:55:41 +0000},
