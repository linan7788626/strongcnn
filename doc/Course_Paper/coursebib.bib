	Arxivurl = {http://arXiv.org/abs/1409.2885},
	Author = {{Zhang}, Y. and {McKay}, T.~A. and {Bertin}, E. and {Jeltema}, T. and {Miller}, C.~J. and {Rykoff}, E. and {Song}, J.},
	Date-Added = {2014-12-11 09:10:23 +0000},
