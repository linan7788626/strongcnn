	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1210.8201},
	Author = {{Miller}, L. and {Heymans}, C. and {Kitching}, T.~D. and {van Waerbeke}, L. and {Erben}, T. and {Hildebrandt}, H. and {Hoekstra}, H. and {Mellier}, Y. and {Rowe}, B.~T.~P. and {Coupon}, J. and {Dietrich}, J.~P. and {Fu}, L. and {Harnois-D{\'e}raps}, J. and {Hudson}, M.~J. and {Kilbinger}, M. and {Kuijken}, K. and {Schrabback}, T. and {Semboloni}, E. and {Vafaei}, S. and {Velander}, M.},
	Date-Added = {2014-10-15 18:08:44 +0000},
