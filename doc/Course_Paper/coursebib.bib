	Adsurl = {http://adsabs.harvard.edu/abs/2011ApJ...735..118R},
	Archiveprefix = {arXiv},
	Author = {{Rozo}, E. and {Wu}, H.-Y. and {Schmidt}, F.},
	Date-Added = {2012-06-26 17:44:15 -0400},
