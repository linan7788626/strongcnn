	Arxivurl = {http://arxiv.org/abs/1502.03824v1},
	Author = {{Graham}, Peter~W. and {Rajendran}, Surjeet and {Tilburg}, Ken~Van and {Wiser}, Timothy~D.},
	Date-Added = {2015-02-16 05:42:45 +0000},
