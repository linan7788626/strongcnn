	Bdsk-Url-1 = {http://dx.doi.org/10.1051/0004-6361/201322177},
	Bdsk-Url-2 = {http://arXiv.org/abs/1312.3948},
	Bdsk-Url-3 = {http://adsabs.harvard.edu/abs/2014A%26A...561A..88E}}

