	Author = {{Mason}, Charlotte~A. and {Treu}, Tommaso and {Schmidt}, Kasper~B. and {Collett}, Thomas~E. and {Trenti}, Michele and {Marshall}, Philip~J. and {Barone-Nugent}, Robert and {Bradley}, Larry~D. and {Stiavelli}, Massimo and {Wyithe}, Stuart},
	Date-Added = {2015-02-14 07:03:59 +0000},
