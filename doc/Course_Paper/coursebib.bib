	Archiveprefix = {arXiv},
	Author = {{Johnston}, D.~E. and {Sheldon}, E.~S. and {Wechsler}, R.~H. and {Rozo}, E. and {Koester}, B.~P. and {Frieman}, J.~A. and {McKay}, T.~A. and {Evrard}, A.~E. and {Becker}, M.~R. and {Annis}, J.},
	Date-Added = {2012-01-03 19:19:43 -0500},
