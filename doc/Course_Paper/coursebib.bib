@article{Sandin:2014aa,
	Adscomment = {17 pages, 9 figures, Astronomy & Astrophysics, in press},
	Adsurl = {http://adsabs.harvard.edu/cgi-bin/bib_query?arXiv:1406.5508},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1406.5508v1},
	Author = {{Sandin}, Christer},
	Date-Added = {2014-06-24 04:24:49 +0000},
