@article{Weinstein:2013ac,
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2013arXiv1310.6541W},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1310.6541},
	Author = {{Weinstein}, G.},
	Date-Added = {2014-01-17 05:27:25 +0000},
