	Adsurl = {http://adsabs.harvard.edu/abs/2015arXiv150106759H},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1501.06759},
	Author = {{Hoyle}, B. and {Rau}, M.~M. and {Bonnett}, C. and {Seitz}, S. and {Weller}, J.},
	Date-Added = {2015-01-28 18:38:24 +0000},
