	Bdsk-Url-1 = {http://arXiv.org/abs/1303.5063},
	Bdsk-Url-2 = {http://adsabs.harvard.edu/abs/2013arXiv1303.5063P}}

@article{Planck-Collaboration:2013ax,
	Adscomment = {56 pages; LaTeX; 42 figures},
