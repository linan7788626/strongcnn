	Arxivurl = {http://arXiv.org/abs/1303.4391},
	Author = {{Kazin}, E.~A. and {S{\'a}nchez}, A.~G. and {Cuesta}, A.~J. and {Beutler}, F. and {Chuang}, C.-H. and {Eisenstein}, D.~J. and {Manera}, M. and {Padmanabhan}, N. and {Percival}, W.~J. and {Prada}, F. and {Ross}, A.~J. and {Seo}, H.-J. and {Tinker}, J. and {Tojeiro}, R. and {Xu}, X. and {Brinkmann}, J. and {Joel}, B. and {Nichol}, R.~C. and {Schlegel}, D.~J. and {Schneider}, D.~P. and {Thomas}, D.},
	Date-Added = {2014-06-18 20:14:56 +0000},
