	Bdsk-Url-2 = {http://arXiv.org/abs/1205.0474},
	Bdsk-Url-3 = {http://adsabs.harvard.edu/abs/2012PhRvD..86l3008B}}

@article{Choudhury:2012aa,
	Adscomment = {28 pages, 3 figures},
