	Adsurl = {http://adsabs.harvard.edu/abs/2015MNRAS.447.2170K},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1502.00008},
	Author = {{K{\"u}ng}, R. and {Saha}, P. and {More}, A. and {Baeten}, E. and {Coles}, J. and {Cornen}, C. and {Macmillan}, C. and {Marshall}, P. and {More}, S. and {Odermatt}, J. and {Verma}, A. and {Wilcox}, J.~K.},
