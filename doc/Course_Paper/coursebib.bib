@article{Ford:2014aa,
	Adscomment = {3D-MF cluster catalog is NOW AVAILABLE at cfhtlens.org. Magnification-shear mass comparison in Figure 9. 17 pages, 9 figures. Submitted to MNRAS},
	Adsurl = {http://adsabs.harvard.edu/cgi-bin/bib_query?arXiv:1409.3571},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1409.3571v1},
	Author = {{Ford}, Jes and {Waerbeke}, Ludovic~Van and {Milkeraitis}, Martha and {Laigle}, Clotilde and {Hildebrandt}, Hendrik and {Erben}, Thomas and {Heymans}, Catherine and {Hoekstra}, Henk and {Kitching}, Thomas and {Mellier}, Yannick and {Miller}, Lance and {Choi}, Ami and {Coupon}, Jean and {Fu}, Liping and {Hudson}, Michael~J. and {Kuijken}, Konrad and {Robertson}, Naomi and {Rowe}, Barnaby and {Schrabback}, Tim and {Velander}, Malin},
	Date-Added = {2014-09-15 01:45:27 +0000},
