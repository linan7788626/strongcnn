@article{Velander:2011aa,
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2011MNRAS.412.2665V},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1011.3041},
	Author = {{Velander}, M. and {Kuijken}, K. and {Schrabback}, T.},
	Date-Added = {2014-05-22 20:28:07 +0000},
	Date-Modified = {2014-05-22 20:28:07 +0000},
	Doi = {10.1111/j.1365-2966.2010.18085.x},
	Eprint = {1011.3041},
