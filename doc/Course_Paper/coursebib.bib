	Bdsk-Url-1 = {http://arXiv.org/abs/1407.1906},
	Bdsk-Url-2 = {http://adsabs.harvard.edu/abs/2014arXiv1407.1906M}}

@article{Hoyle:2014aa,
	Adscomment = {10 pages, 4 figures, submitted to MNRAS},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2014arXiv1410.4696H},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1410.4696},
	Author = {{Hoyle}, B. and {Rau}, M.~M. and {Zitlau}, R. and {Seitz}, S. and {Weller}, J.},
