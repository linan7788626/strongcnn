	Archiveprefix = {arXiv},
	Author = {{Leauthaud}, A. and {Finoguenov}, A. and {Kneib}, J.-P. and {Taylor}, J.~E. and {Massey}, R. and {Rhodes}, J. and {Ilbert}, O. and {Bundy}, K. and {Tinker}, J. and {George}, M.~R. and {Capak}, P. and {Koekemoer}, A.~M. and {Johnston}, D.~E. and {Zhang}, Y.-Y. and {Cappelluti}, N. and {Ellis}, R.~S. and {Elvis}, M. and {Giodini}, S. and {Heymans}, C. and {Le F{\`e}vre}, O. and {Lilly}, S. and {McCracken}, H.~J. and {Mellier}, Y. and {R{\'e}fr{\'e}gier}, A. and {Salvato}, M. and {Scoville}, N. and {Smoot}, G. and {Tanaka}, M. and {Van Waerbeke}, L. and {Wolk}, M.},
	Date-Added = {2012-03-08 03:03:25 -0500},
