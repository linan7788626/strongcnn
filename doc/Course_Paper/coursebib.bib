	Bdsk-Url-1 = {http://arxiv.org/abs/1410.4270v1},
	Bdsk-Url-2 = {http://adsabs.harvard.edu/cgi-bin/bib_query?arXiv:1410.4270}}

@article{Akeret:2014aa,
	Adscomment = {Submitted to Astronomy and Computing. 13 pages, 1 figure. The code is available at http://hope.phys.ethz.ch},
	Adsurl = {http://adsabs.harvard.edu/cgi-bin/bib_query?arXiv:1410.4345},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1410.4345v1},
	Author = {{Akeret}, Joel and {Gamper}, Lukas and {Amara}, Adam and {Refregier}, Alexandre},
	Date-Added = {2014-10-17 02:50:13 +0000},
	Date-Modified = {2014-10-17 02:50:13 +0000},
	Eprint = {1410.4345v1},
	Jornal = {ArXiv e-prints},
	Month = {Oct},
	Primaryclass = {astro-ph.IM},
	Title = {HOPE: A Python Just-In-Time compiler for astrophysical computations},
	Year = {2014},
	Abstract = {The Python programming language is becoming increasingly popular for scientific applications due to its simplicity, versatility, and the broad range of its libraries. A drawback of this dynamic language, however, is its low runtime performance which limits its applicability for large simulations and for the analysis of large data sets, as is common in astrophysics and cosmology. While various frameworks have been developed to address this limitation, most focus on covering the complete language set, and either force the user to alter the code or are not able to reach the full speed of an optimised native compiled language. In order to combine the ease of Python and the speed of C++, we developed HOPE, a specialised Python just-in-time (JIT) compiler designed for numerical astrophysical applications. HOPE focuses on a subset of the language and is able to translate Python code into C++ while performing numerical optimisation on mathematical expressions at runtime. To enable the JIT compilation, the user only needs to add a decorator to the function definition. We assess the performance of HOPE by performing a series of benchmarks and compare its execution speed with that of plain Python, C++ and the other existing frameworks. We find that HOPE improves the performance compared to plain Python by a factor of 2 to 120, achieves speeds comparable to that of C++, and often exceeds the speed of the existing solutions. We discuss the differences between HOPE and the other frameworks, as well as future extensions of its capabilities. The fully documented HOPE package is available at http://hope.phys.ethz.ch and is published under the GPLv3 license on PyPI and GitHub.},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://arxiv.org/abs/1410.4345v1},
	Bdsk-Url-2 = {http://adsabs.harvard.edu/cgi-bin/bib_query?arXiv:1410.4345}}

@article{Cornish:2014aa,
	Adscomment = {34 pages, 15 figures},
	Adsurl = {http://adsabs.harvard.edu/cgi-bin/bib_query?arXiv:1410.3835},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1410.3835v1},
	Author = {{Cornish}, Neil~J. and {Littenberg}, Tyson~B.},
	Date-Added = {2014-10-16 17:24:40 +0000},
	Date-Modified = {2014-10-16 17:24:41 +0000},
	Eprint = {1410.3835v1},
	Jornal = {ArXiv e-prints},
	Month = {Oct},
	Primaryclass = {gr-qc},
	Title = {BayesWave: Bayesian Inference for Gravitational Wave Bursts and Instrument Glitches},
	Year = {2014},
	Abstract = {A central challenge in Gravitational Wave Astronomy is identifying weak signals in the presence of non-stationary and non-Gaussian noise. The separation of gravitational wave signals from noise requires good models for both. When accurate signal models are available, such as for binary Neutron star systems, it is possible to make robust detection statements even when the noise is poorly understood. In contrast, searches for "un-modeled" transient signals are strongly impacted by the methods used to characterize the noise. Here we take a Bayesian approach and introduce a multi-component, variable dimension, parameterized noise model that explicitly accounts for non-stationarity and non-Gaussianity in data from interferometric gravitational wave detectors. Instrumental transients (glitches) and burst sources of gravitational waves are modeled using a Morlet-Gabor continuous wavelet basis. The number and placement of the wavelets is determined by a trans-dimensional Reversible Jump Markov Chain Monte Carlo algorithm. The Gaussian component of the noise and sharp line features in the noise spectrum are modeled using the BayesLine algorithm, which operates in concert with the wavelet model.},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://arxiv.org/abs/1410.3835v1},
	Bdsk-Url-2 = {http://adsabs.harvard.edu/cgi-bin/bib_query?arXiv:1410.3835}}

@article{Peiris:2014aa,
	Adscomment = {An informal summary of an invited talk given at the IAU symposium 'Statistical Challenges in 21st Century Cosmology', accepted in the Proceedings IAU Symposium No. 306, 2014 (A.F. Heavens, J.-L. Starck, A. Krone-Martins eds.). 7 pages, 1 figure. Comments welcome},
	Adsurl = {http://adsabs.harvard.edu/cgi-bin/bib_query?arXiv:1410.3837},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1410.3837v1},
	Author = {{Peiris}, Hiranya~V.},
	Date-Added = {2014-10-16 17:24:35 +0000},
	Date-Modified = {2014-10-16 17:24:36 +0000},
	Eprint = {1410.3837v1},
	Jornal = {ArXiv e-prints},
	Month = {Oct},
	Primaryclass = {astro-ph.CO},
	Title = {Considerations in the Interpretation of Cosmological Anomalies},
	Year = {2014},
	Abstract = {Anomalies drive scientific discovery -- they are associated with the cutting edge of the research frontier, and thus typically exploit data in the low signal-to-noise regime. In astronomy, the prevalence of systematics --- both "known unknowns" and "unknown unknowns" --- combined with increasingly large datasets, the widespread use of ad hoc estimators for anomaly detection, and the "look-elsewhere" effect, can lead to spurious false detections. In this informal note, I argue that anomaly detection leading to discoveries of new physics requires a combination of physical understanding, careful experimental design to avoid confirmation bias, and self-consistent statistical methods. These points are illustrated with several concrete examples from cosmology.},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://arxiv.org/abs/1410.3837v1},
	Bdsk-Url-2 = {http://adsabs.harvard.edu/cgi-bin/bib_query?arXiv:1410.3837}}

@article{Hwang:2014aa,
	Adscomment = {22 pages, 19 figures, 2 tables. To appear in ApJ. Paper with high resolution figures is available at http://astro.kias.re.kr/~hshwang/ms_hwang20141014.pdf},
	Adsurl = {http://adsabs.harvard.edu/cgi-bin/bib_query?arXiv:1410.3883},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1410.3883v1},
	Author = {{Hwang}, Ho~Seong and {Geller}, Margaret~J. and {Diaferio}, Antonaldo and {Rines}, Kenneth~J. and {Zahid}, H.~Jabran},
	Date-Added = {2014-10-16 17:24:35 +0000},
