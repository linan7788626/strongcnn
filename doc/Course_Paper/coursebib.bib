	Arxivurl = {http://arXiv.org/abs/0908.2033},
	Author = {{Banerji}, M. and {Lahav}, O. and {Lintott}, C.~J. and {Abdalla}, F.~B. and {Schawinski}, K. and {Bamford}, S.~P. and {Andreescu}, D. and {Murray}, P. and {Raddick}, M.~J. and {Slosar}, A. and {Szalay}, A. and {Thomas}, D. and {Vandenberg}, J.},
	Date-Added = {2014-05-29 09:19:21 +0000},
