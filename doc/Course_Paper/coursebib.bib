	Author = {{Kobayashi}, M.~I.~N. and {Leauthaud}, A. and {More}, S. and {Okabe}, N. and {Laigle}, C. and {Rhodes}, J. and {Takeuchi}, T.~T.},
	Date-Added = {2015-03-03 23:00:14 +0000},
