@article{Valageas:2014aa,
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2014A%26A...561A..53V},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1306.6151},
	Author = {{Valageas}, P.},
	Date-Added = {2014-06-18 20:02:51 +0000},
