	Keywords = {DWARF GALAXIES, GRAVITATIONAL LENSES, IMAGE RESOLUTION, MASS DISTRIBUTION, SIGNAL DISTORTION, CROSS CORRELATION, ERROR ANALYSIS, FUNCTIONS (MATHEMATICS), IMAGE PROCESSING, LUMINOSITY},
