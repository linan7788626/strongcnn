	Author = {{Albareti}, Franco~D. and {Comparat}, Johan and {Guti{\'e}rrez}, Carlos~M. and {Prada}, Francisco and {P{\^a}ris}, Isabelle and {Schlegel}, David and {L{\'o}pez-Corredoira}, Mart{\'\i}n and {Schneider}, Donald~P. and {Manchado}, Arturo and {Garc{\'\i}a-Hern{\'a}ndez}, D.~A. and {Petitjean}, Patrick and {Ge}, Jian},
	Date-Added = {2015-01-06 03:07:06 +0000},
