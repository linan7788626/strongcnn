	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1501.06893},
	Author = {{Wiesner}, M.~P. and {Lin}, H. and {Soares-Santos}, M.},
	Date-Added = {2015-01-28 18:38:29 +0000},
