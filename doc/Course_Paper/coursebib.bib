	Arxivurl = {http://arXiv.org/abs/1403.1063},
	Author = {{Joseph}, R. and {Courbin}, F. and {Metcalf}, R.~B. and {Giocoli}, C. and {Hartley}, P. and {Jackson}, N. and {Bellagamba}, F. and {Kneib}, J.-P. and {Koopmans}, L. and {Lemson}, G. and {Meneghetti}, M. and {Meylan}, G. and {Petkova}, M. and {Pires}, S.},
	Date-Added = {2014-03-06 19:57:02 +0000},
