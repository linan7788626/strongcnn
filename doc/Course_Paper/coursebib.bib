	Doi = {10.1086/170592},
	Journal = {\apj},
	Keywords = {ASTRONOMICAL SPECTROSCOPY, GALACTIC CLUSTERS, GALACTIC EVOLUTION, GRAVITATIONAL EFFECTS, ABSORPTION SPECTRA, ARCS, EMISSION SPECTRA, LYMAN ALPHA RADIATION, RED SHIFT, STAR FORMATION RATE},
	Month = oct,
	Pages = {334-343},
	Read = {1},
	Readabstract = {1},
	Title = {{Spectroscopy of the gravitational arcs in CL 2244 - 02, A370 (arclet A5), and CL 0024 + 1654}},
	Volume = 380,
	Year = 1991,
	Abstract = {Recent spectroscopic data on the blue giant arc in Cl 2244 - 02, on the arclet A5 in Abell 370, and on the split arc in Cl 0024 + 1654 are analyzed. A probable redshift of 2.237 is derived for the source of the Cl 2244 - 02 arc, making this object one of the most distant 'field' galaxies known to date. A star formation rate is estimated from the Ly-alpha intensity and compared to other distant field galaxies. For A5, no prominent emission line is found, but three absorption lines indicate a tentative redshift of 1.306. The spectra of the four parts of the Cl 0024 + 1654 arc look quite similar, but the absence of any spectral feature allows only the determination of a redshift range between 1 and 2.1.},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1086/170592}}

@article{Fort:1988aa,
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/1988A%26A...200L..17F},
	Author = {{Fort}, B. and {Prieur}, J.~L. and {Mathez}, G. and {Mellier}, Y. and {Soucail}, G.},
	Date-Added = {2012-03-10 14:22:47 -0500},
	Date-Modified = {2014-09-23 02:09:52 +0000},
	Journal = {\aap},
	Keywords = {ASTRONOMICAL PHOTOMETRY, GALACTIC CLUSTERS, GALACTIC NUCLEI, GALACTIC STRUCTURE, GRAVITATIONAL LENSES, RED SHIFT, ARCS, ASTRONOMICAL PHOTOGRAPHY, CHARGE COUPLED DEVICES, DARK MATTER, GALACTIC RADIATION, HIGH RESOLUTION},
	Month = jul,
	Pages = {L17-L20},
	Read = {1},
	Readabstract = {1},
	Title = {{Faint distorted structures in the core of A 370 - Are they gravitationally lensed galaxies at Z about 1?}},
	Volume = 200,
	Year = 1988,
	Abstract = {This letter presents the analysis of deep and high resolution CCD images of the field of the cluster of galaxies Abell 370 (z = 0.374). The giant arc near the center was recently identified as the image of a distant galaxy gravitationally lensed by the cluster core. No counterarc is found where expected, but several faint and elongated objects are detected in the field. By far bluer than all cluster members, these structures are probably gravitationally lensed, distorted and amplified background galaxies at redshift z about 1. This reinforces the hypothesis that both dark and visible matter are essentially concentrated in the core of clusters of galaxies and act as gravitational lenses. From this observation, it is predicted that many such very distant and early galaxies may be detected in the near future from deep photometry of the cores of centrally concentrated clusters at intermediate redshift (z about 0.5).},
	Bdsk-File-1 = {}}

