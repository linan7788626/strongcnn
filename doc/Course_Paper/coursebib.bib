	Readabstract = {1},
	Series = {Bulletin of the American Astronomical Society},
	Title = {{The Mass-Luminosity Relation of Galaxies in the Deep Lens Survey}},
	Volume = 43,
	Year = 2011,
	Abstract = {Characterization of the redshift-dependence of the total mass to light and stellar mass to total mass relationships is an important probe of galaxy evolution. Recent theoretical progress has been made using statistical models such as abundance matching, but observational studies have been primarily limited to galaxies at low redshifts or very massive galaxies at higher redshifts. We present results from the Deep Lens Survey using galaxy-galaxy weak lensing to directly constrain total galaxy masses over several decades of mass out to z 0.7. We investigate the correlation of the M/L with color and environment as well as the possibility of evolution with cosmological time.}}

@inproceedings{DellAntonio:2011aa,
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2011AAS...21725223D},
	Author = {{Dell'Antonio}, I.~P. and {Wittman}, D. and {Jain}, B. and {Bosch}, J. and {Clowe}, D. and {Jarvis}, M. and {Jee}, M. and {Tyson}, J. and {Zhan}, H. and {LSST Weak Lensing Science Collaboration}},
	Booktitle = {American Astronomical Society Meeting Abstracts \#217},
