	Bdsk-Url-1 = {http://arxiv.org/abs/1502.03888v1},
	Bdsk-Url-2 = {http://adsabs.harvard.edu/cgi-bin/bib_query?arXiv:1502.03888}}

@article{Graham:2015ab,
	Adscomment = {16 pages, 7 figures, plus appendix},
	Adsurl = {http://adsabs.harvard.edu/cgi-bin/bib_query?arXiv:1502.03824},
