@article{Sijacki:2014aa,
	Adscomment = {22 pages, 15 figures. MNRAS submitted. The Illustris website can be found at http://www.illustris-project.org/},
	Adsurl = {http://adsabs.harvard.edu/cgi-bin/bib_query?arXiv:1408.6842},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1408.6842v1},
	Author = {{Sijacki}, Debora and {Vogelsberger}, Mark and {Genel}, Shy and {Springel}, Volker and {Torrey}, Paul and {Snyder}, Greg and {Nelson}, Dylan and {Hernquist}, Lars},
	Date-Added = {2014-09-01 05:00:12 +0000},
